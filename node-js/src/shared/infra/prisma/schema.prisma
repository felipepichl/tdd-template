generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Users {
  id         String   @id @default(uuid())
  name       String
  email      String   @unique
  password   String
  avatar     String?
  created_at DateTime @default(now())

  @@map("users")
}
